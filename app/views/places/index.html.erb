<h1>Places</h1>

<div class="list-group mb-3">
  <% for place in @places %>
    <a href="/places/<%= place["id"] %>" class="list-group-item list-group-item-action list-group-item-light">
      <%= place["name"] %>
    </a>
  <% end %>
</div>

<p>
  <a href="/places/new" class="btn btn-primary">New Place</a>
</p>
